<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Multibrowsertesting" parallel="tests">
  <test name="ChromeTest">
  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="testscript.LoginTest">
      <methods>
    <!--  <include name="verifyuserisabletologinusingvalidcredentials"></include>
      <include name="verifyuserisunabletologinusinginvalidusernameandvalidpassword"></include> -->
      <exclude name="verifyuserisabletologinusingvalidcredentials"></exclude>
      <exclude name="verifyuserisunabletologinusinginvalidusernameandvalidpassword"></exclude>
      </methods>
      </class>
    </classes>
  </test> <!-- Test -->
  <test name="FirefoxTest">
  <parameter name="browser" value="firefox"></parameter>
    <classes>
      <class name="testscript.LoginTest">
      <methods>
    <!--  <include name="verifyuserisabletologinusingvalidcredentials"></include>
      <include name="verifyuserisunabletologinusinginvalidusernameandvalidpassword"></include> -->
      <exclude name="verifyuserisabletologinusingvalidcredentials"></exclude>
      <exclude name="verifyuserisunabletologinusinginvalidusernameandvalidpassword"></exclude>
      </methods>
      </class>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
